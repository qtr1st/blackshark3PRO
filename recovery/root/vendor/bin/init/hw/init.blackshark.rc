# add for slidekeys
on boot
    chown system system /dev/motor_0
    chown system system /sys/motor_0/move
    chmod 0664 /dev/motor_0
    chmod 0664 /sys/motor_0/move

    chown system system /dev/hall_0
    chown system system /dev/hall_1
